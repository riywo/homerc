export PATH="$HOME/usr/local/bin:$PATH"
alias git='GIT_SSL_NO_VERIFY=1 git'

source "usr/local/share/git-completion/git-prompt.sh"
source "usr/local/share/git-completion/git-completion.bash"
export PS1='[\u@\h \W$(__git_ps1 " (%s)")]\$ '
export PS1='\[\033k'"$HOSTNAME"'\033\\\]'"$PS1"

export MANPATH="$HOME/usr/local/share/man:$HOME/usr/local/man:$MANPATH"
